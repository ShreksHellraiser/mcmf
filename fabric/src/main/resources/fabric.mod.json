{
  "schemaVersion": 1,
  "id": "mcmf",
  "version": "${version}",
  "name": "Minecraft Mainframes",
  "description": "This is a computer mod which adds programmable UXN computers to the game.",
  "authors": [
    "ShreksHellraiser"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/mcmf/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.shrekshellraiser.fabric.ComputerModFabric"
    ],
    "client": [
      "com.github.shrekshellraiser.fabric.client.ComputerModFabricClient"
    ],
    "fabric-datagen": [
      "com.github.shrekshellraiser.fabric.datagen.DataGenerator"
    ]
  },
  "mixins": [
    "mcmf.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "architectury": ">=4.12.94",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
