{
  "multipart": [
    {
      "when": {
        "cable_north": "false",
        "cable_south": "false",
        "cable_east": "false",
        "cable_west": "false"
      },
      "apply": {
        "model": "mcmf:block/cable/cable"
      }
    },
    {
      "when": {
        "cable_north": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_n"
      }
    },
    {
      "when": {
        "cable_east": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_e"
      }
    },
    {
      "when": {
        "cable_south": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_s"
      }
    },
    {
      "when": {
        "cable_west": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_w"
      }
    },
    {
      "when": {
        "connector_north": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector"
      }
    },
    {
      "when": {
        "connector_east": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector",
        "y": 90
      }
    },
    {
      "when": {
        "connector_south": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector",
        "y": 180
      }
    },
    {
      "when": {
        "connector_west": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector",
        "y": 270
      }
    }
  ]
}
