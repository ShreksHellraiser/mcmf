{
  "multipart": [
    {
      "when": {
        "cable_north": "false",
        "cable_south": "false",
        "cable_east": "false",
        "cable_west": "false",
        "wall": "down"
      },
      "apply": {
        "model": "mcmf:block/cable/cable"
      }
    },
    {
      "when": {
        "cable_north": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_n"
      }
    },
    {
      "when": {
        "cable_east": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_e"
      }
    },
    {
      "when": {
        "cable_south": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_s"
      }
    },
    {
      "when": {
        "cable_west": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_w"
      }
    },
    {
      "when": {
        "connector_north": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector"
      }
    },
    {
      "when": {
        "connector_east": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector",
        "y": 90
      }
    },
    {
      "when": {
        "connector_south": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector",
        "y": 180
      }
    },
    {
      "when": {
        "connector_west": "true"
      },
      "apply": {
        "model": "mcmf:block/cable/connector",
        "y": 270
      }
    },
    {
      "when": {
        "wall": "north"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_wall"
      }
    },
    {
      "when": {
        "wall": "east"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_wall",
        "y": 90
      }
    },
    {
      "when": {
        "wall": "south"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_wall",
        "y": 180
      }
    },
    {
      "when": {
        "wall": "west"
      },
      "apply": {
        "model": "mcmf:block/cable/cable_wall",
        "y": 270
      }
    },
    {
      "when": {
        "connector_extended": "true",
        "wall": "north"
      },
      "apply": {
        "model": "mcmf:block/cable/connector_extended"
      }
    },
    {
      "when": {
        "connector_extended": "true",
        "wall": "east"
      },
      "apply": {
        "model": "mcmf:block/cable/connector_extended",
        "y": 90
      }
    },
    {
      "when": {
        "connector_extended": "true",
        "wall": "south"
      },
      "apply": {
        "model": "mcmf:block/cable/connector_extended",
        "y": 180
      }
    },
    {
      "when": {
        "connector_extended": "true",
        "wall": "west"
      },
      "apply": {
        "model": "mcmf:block/cable/connector_extended",
        "y": 270
      }
    }
  ]
}
